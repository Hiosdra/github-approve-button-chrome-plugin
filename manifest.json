{
  "manifest_version": 2,
  "name": "GitHub PR Approve Button",
  "version": "1.0",
  "description": "Adds a button to approve pull requests on GitHub.",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*/pull/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
